package DataStructures;

public class Excep {
	public static void main(String[] args) {
		try {
			int[] myNumbers = { 1, 2, 3 };
			System.out.println(myNumbers[1]);
		} catch (Exception e) {
			System.out.println("Some thing wemt wrong");
		} finally {
			System.out.println("The 'try catch' is finished");
		}
	}

}
